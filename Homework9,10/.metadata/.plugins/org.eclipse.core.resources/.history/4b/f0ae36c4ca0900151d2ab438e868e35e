package prog3;

public class Test {

	public static void main(String[] args) {
		Library library = new Library("Library", "London");
		Book book1 = new Book("book 1", "author 1", 300);
		library.addBook(book1);
		library.addBook(book1);
		library.addBook(book1);
		library.bookCount(book1);
		
		library.removeBook(book1);
		library.removeBook(book1);
		library.bookCount(book1);
		
		library.removeBook(book1);
		library.bookCount(book1);
	}

}