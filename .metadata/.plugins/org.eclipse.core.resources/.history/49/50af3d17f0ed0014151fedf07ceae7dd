package lekciq2;
import java.util.Scanner;
public class Three {

	public static void main(String[] args) {
		
		Scanner input= new Scanner(System.in,"UTF-8");
		System.out.println("Name|Surname|Years|Gender|CitizenNumber|Telephone");
		
		System.out.println("Enter First name:");
		String name=input.nextLine();

		System.out.println("Enter Surname:");
		String familyName=input.nextLine();
		
		System.out.println("Enter years:");
		int years = input.nextInt();
		
		input.nextLine();
		
		System.out.println("Enter gender: ");
		char gender=input.nextChar();
		
		System.out.println("Enter ID: ");
		long citNum=input.nextLong();
		
		System.out.println("Enter Telephone: ");
		String phone=input.nextLine(); 
		
		System.out.println("Persons name is: " +name +" Surname is: " +familyName +" Years are: " +years +" Gender is: " +gender +" Citizen number is: " +citNum +" The phone is: " +phone);
		input.close();
	}

}